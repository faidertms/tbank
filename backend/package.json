    "pg": "^8.5.1",
    "knex": "^0.21.19",
    "objection": "^2.2.3"
  },
  "devDependencies": {
    "@types/knex": "^0.16.1",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
